#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.17_@vitest+ui@3.2.4_jiti@2.6.0_lightningcss@1.30.1_yaml@2.8.1/node_modules/vitest/node_modules:/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.17_@vitest+ui@3.2.4_jiti@2.6.0_lightningcss@1.30.1_yaml@2.8.1/node_modules:/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.17_@vitest+ui@3.2.4_jiti@2.6.0_lightningcss@1.30.1_yaml@2.8.1/node_modules/vitest/node_modules:/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/vitest@3.2.4_@types+debug@4.1.12_@types+node@20.19.17_@vitest+ui@3.2.4_jiti@2.6.0_lightningcss@1.30.1_yaml@2.8.1/node_modules:/home/vishesh.baghel/Documents/workspace/portfolio/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
